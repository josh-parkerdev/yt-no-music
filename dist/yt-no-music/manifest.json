{
  "name": "YT No Music",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://*.youtube.com/*"
      ]
    }
  ]
}